<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogInFormController">
   <children>
      <Rectangle arcHeight="20.0" arcWidth="20.0" fill="#1f93ff00" height="163.0" layoutX="60.0" layoutY="80.0" stroke="BLACK" strokeType="INSIDE" width="480.0" />
      <VBox layoutX="78.0" layoutY="103.0" prefHeight="232.0" prefWidth="445.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="445.0">
               <children>
                  <Label fx:id="userIDLabel" prefHeight="17.0" prefWidth="200.0" text="User ID:" />
                  <TextField fx:id="userIDInput" alignment="TOP_LEFT">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin></TextField>
               </children></HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="50.0" prefWidth="445.0">
               <children>
                  <Label fx:id="passwordLabel" prefHeight="17.0" prefWidth="200.0" text="Password:" />
                  <TextField fx:id="passwordInput">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                  </TextField>
               </children></HBox>
            <HBox layoutX="10.0" layoutY="110.0" prefHeight="50.0" prefWidth="445.0">
               <children>
                  <Label fx:id="locationLabel" prefHeight="17.0" prefWidth="200.0" text="Location:" />
                  <Label fx:id="zoneID" text="Label">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                  </Label>
               </children></HBox>
            <HBox layoutX="10.0" layoutY="146.0" prefHeight="50.0" prefWidth="445.0">
               <children>
                  <Button fx:id="enterButton" alignment="CENTER" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#onActionEnterInputData" prefHeight="25.0" prefWidth="50.0" text="Enter">
                     <HBox.margin>
                        <Insets left="275.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="exitButton" alignment="CENTER" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#onActionExitLogIn" prefHeight="25.0" prefWidth="50.0" text="Exit">
                     <HBox.margin>
                        <Insets left="25.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children></VBox>
      <Label fx:id="logInLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="60.0" layoutY="46.0" text="Log-In Form">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
